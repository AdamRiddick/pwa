{
  "AllowedHosts": "*",
  "ApiSettings": {
    "Proxies": [
      {
        "forwardingUrl": "https://localhost:5001",
        "path": "/api"
      }
    ]
  },
  "CSP": {
    "ReportTo": [
      {
        "group": "report-uri",
        "max_age": 10886400,
        "endpoints": [
          {
            "url": "[Redacted]"
          }
        ],
        "include_subdomains": true
      }
    ]
  },
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
